if [ -e ~/.bashrc  ]
then
    . ~/.bashrc
fi

WORKSHOP=~/Workshop

MVN_HOME=${WORKSHOP}/apache-maven-3.2.3

JAVA6_HOME=/System/Library/Java/JavaVirtualMachines/1.6.0.jdk/Contents/Home
JAVA7_HOME=/Library/Java/JavaVirtualMachines/jdk1.7.0_60.jdk/Contents/Home
JAVA8_HOME=/Library/Java/JavaVirtualMachines/jdk1.8.0_73.jdk/Contents/Home
export JAVA_HOME=${JAVA8_HOME}

export CATALINA_HOME=${WORKSHOP}/apache-tomcat-7.0.57

export PATH=/usr/local/bin:${ANT_HOME}/bin:${MVN_HOME}/bin:$PATH 

# enable vi mode
set -o vi # set -o emacs is to enable emacs default mode

# go lang
export GOPATH=~/Developer/go
export PATH=${GOPATH}/bin:$PATH

# android sdk
export ANDROID_HOME=/usr/local/opt/android-sdk
export PATH=$PATH:${ANDROID_HOME}/tools:${ANDROID_HOME}/platform-tools

# Triton ssh aliases to dev machines on RDMZ
alias sshCIE='ssh -i ~/.ssh/id_rsa -2 -X wan119@cielo.bmi.osumc.edu'
alias sshCIEDEV='ssh -i ~/.ssh/id_rsa -2 -X wan119@cielo-dev.bmi.osumc.edu'
alias sshJenkins='ssh -i ~/.ssh/id_rsa -2 -X wan119@dashboard.bmi.osumc.edu'
alias sshNexus='ssh -i ~/.ssh/id_rsa -2 -X nexus@repo.bmi.osumc.edu'


#THIS MUST BE AT THE END OF THE FILE FOR SDKMAN TO WORK!!!
export SDKMAN_DIR="/Users/swang/.sdkman"
[[ -s "/Users/swang/.sdkman/bin/sdkman-init.sh" ]] && source "/Users/swang/.sdkman/bin/sdkman-init.sh"
