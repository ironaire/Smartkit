alias sshTritonTS='ssh -i ~/.ssh/wan119 -2 -X cagrid@triton-tissue.bmi.osumc.edu'
alias sshCSM='ssh -i ~/.ssh/wan119 -2 -X cagrid@csm.bmi.osumc.edu'
alias sshRIEQP='ssh -i ~/.ssh/wan119 -2 -X wan119@hcl-qp-dev.bmi.osumc.edu'
