# theme options

# aliases
alias gc='/Users/swang/.sdkman/candidates/groovy/current/bin/groovyc'
alias gy='/Users/swang/.sdkman/candidates/groovy/current/bin/groovy'

## other useful tools
if type nvim > /dev/null 2>&1; then
  alias vim='nvim'
fi

#THIS MUST BE AT THE END OF THE FILE FOR SDKMAN TO WORK!!!
export SDKMAN_DIR="/Users/swang/.sdkman"
[[ -s "/Users/swang/.sdkman/bin/sdkman-init.sh" ]] && source "/Users/swang/.sdkman/bin/sdkman-init.sh"
